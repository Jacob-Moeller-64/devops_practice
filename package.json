{
  "name": "devsecops-demo",
  "version": "1.0.0",
  "description": "DevSecOps pipeline demonstration",
  "main": "app.js",
  "overrides": {
    "cross-spawn": "7.0.6",
    "glob": "11.1.0"
  },
  "scripts": {
    "start": "node app.js",
    "test": "jest",
    "test:coverage": "c8 jest",
    "lint": "eslint ."
  },
  "keywords": [
    "devsecops",
    "nodejs",
    "express"
  ],
  "author": "Jacob",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "c8": "^10.1.2",
    "eslint": "^8.50.0",
    "jest": "^30.2.0",
    "@jest/globals": "^30.2.0",
    "supertest": "^6.3.3"
  },
  "c8": {
    "reporter": [
      "text",
      "lcov",
      "html"
    ],
    "exclude": [
      "coverage/**",
      "node_modules/**",
      "**/*.test.js"
    ]
  }
}
