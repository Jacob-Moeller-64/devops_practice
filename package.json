{
  "name": "devsecops-demo",
  "version": "1.0.0",
  "description": "DevSecOps pipeline demonstration",
  "main": "app.js",
  "overrides": {
    "cross-spawn": "7.0.6",
    "glob": "11.1.0"
  },
  "scripts": {
    "start": "node app.js",
    "test": "jest --coverage",
    "lint": "eslint ."
  },
  "keywords": [
    "devsecops",
    "nodejs",
    "express"
  ],
  "author": "Jacob",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "babel-jest": "^30.2.0",
    "eslint": "^8.50.0",
    "jest": "^30.2.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "*.js",
      "!jest.config.js",
      "!coverage/**"
    ]
  }
}
